{
  "name": "simple-amqplib",
  "version": "7.0.4-1",
  "description": "Dead simple consume & publish for RabbitMQ.",
  "main": "amqp.js",
  "scripts": {
    "coverage": "c8 npm test",
    "docs": "jsdoc2md amqp.js",
    "test": "semistandard && NODE_ENV=test mocha test --recursive --exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noblesamurai/node-simple-amqplib"
  },
  "keywords": [
    "AMQP",
    "AMQP 0-9-1",
    "RabbitMQ",
    "amqplib",
    "consume",
    "publish"
  ],
  "author": "Timothy Leslie Allen <allen.timothy.email@gmail.com> (https://github.com/timlesallen)",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/noblesamurai/node-simple-amqplib/issues"
  },
  "dependencies": {
    "amqplib": "^0.7.1",
    "debug": "^2.6.8",
    "json-stringify-safe": "^5.0.0"
  },
  "devDependencies": {
    "c8": "^7.7.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "jsdoc-to-markdown": "^7.0.1",
    "mocha": "^8.3.2",
    "sandboxed-module": "^0.3.0",
    "semistandard": "^16.0.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.3.0"
  },
  "c8": {
    "all": true,
    "include": [
      "amqp.js",
      "lib"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  }
}
