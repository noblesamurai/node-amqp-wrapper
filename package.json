{
  "name": "amqp-wrapper",
  "version": "5.5.2",
  "description": "A wrapper around https://github.com/squaremo/amqp.node to make consuming and publishing dead easy.",
  "main": "amqp.js",
  "scripts": {
    "test": "semistandard && NODE_ENV=test mocha test --recursive",
    "jshint": "jshint -c .jshintrc --exclude-path .gitignore .",
    "codestyle": "jscs -p google lib/ test/",
    "coverage": "nyc -a -c -r html -r text -r lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noblesamurai/node-amqp-wrapper"
  },
  "keywords": [
    "amqp",
    "wrapper",
    "rabbitmq"
  ],
  "author": "Timothy Leslie Allen <allen.timothy.email@gmail.com> (https://github.com/timlesallen)",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/noblesamurai/node-amqp-wrapper/issues"
  },
  "dependencies": {
    "amqplib": "^0.5.5",
    "async": "^0.9.0",
    "debug": "^2.6.8",
    "deferential": "^1.0.0",
    "json-stringify-safe": "^5.0.0",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "jscs": "^1.6.1",
    "jshint": "^2.8.0",
    "lodash": "^2.4.1",
    "mocha": "^6.2.0",
    "nyc": "^11.4.1",
    "sandboxed-module": "^0.3.0",
    "semistandard": "^12.0.1",
    "sinon": "^1.10.3"
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  }
}
